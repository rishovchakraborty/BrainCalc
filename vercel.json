{
    "version": 2,
    "builds": [
      {
        "src": "mathnode/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "mathnode/build"
        }
      },
      {
        "src": "server/requirements.txt",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/$1" },
      { "src": "/(.*)", "dest": "mathnode/$1" }
    ]
  }
  